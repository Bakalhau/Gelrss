# Gelbooru credentials (OPTIONAL - but recommended to avoid rate limits)
# To obtain, create an account at gelbooru.com and go to Account > Options
GELBOORU_API_KEY=your_gelbooru_api_key
GELBOORU_USER_ID=your_gelbooru_id

# Cache update interval in minutes (default: 10)
UPDATE_INTERVAL_MINUTES=10

# Base URL of your server (IP/domain only, no port)
# Examples: localhost, 127.0.0.1, mydomain.com
BASE_URL=localhost

# Server port
PORT=24454